    <sql>SELECT &#xd;&#xa;    selected_date as date_to_insert,&#xd;&#xa;    EXTRACT&#x28;DAY FROM selected_date&#x29; as day, &#xd;&#xa;    EXTRACT&#x28;WEEK FROM selected_date&#x29; as week, &#xd;&#xa;    &#x28;SELECT&#xd;&#xa;  CASE EXTRACT&#x28;MONTH FROM selected_date&#x29;&#xd;&#xa;     WHEN 1 THEN &#x27;January&#x27;&#xd;&#xa;     WHEN 2 THEN &#x27;February&#x27;&#xd;&#xa;     WHEN 3 THEN &#x27;March&#x27;&#xd;&#xa;     WHEN 4 THEN &#x27;April&#x27;&#xd;&#xa;     WHEN 5 THEN &#x27;May&#x27;&#xd;&#xa;     WHEN 6 THEN &#x27;June&#x27;&#xd;&#xa;     WHEN 7 THEN &#x27;July&#x27;&#xd;&#xa;     WHEN 8 THEN &#x27;August&#x27;&#xd;&#xa;     WHEN 9 THEN &#x27;September&#x27;&#xd;&#xa;     WHEN 10 THEN &#x27;October&#x27;&#xd;&#xa;     WHEN 11 THEN &#x27;November&#x27;&#xd;&#xa;     WHEN 12 THEN &#x27;December&#x27;&#xd;&#xa;     ELSE &#x27;What kind of calendar is it&#x3f;&#x27;&#xd;&#xa;  END&#x29; as month,&#xd;&#xa;    EXTRACT&#x28;QUARTER FROM selected_date&#x29; as quarter,&#xd;&#xa;    &#x28;IF&#x28;MONTH&#x28;selected_date&#x29; &#x3c; 7, 1, 2&#x29;&#x29; as semester, &#xd;&#xa;    EXTRACT&#x28;YEAR FROM selected_date&#x29; as year &#xd;&#xa;FROM &#xd;&#xa;&#x28;&#xd;&#xa;    SELECT adddate&#x28;&#xd;&#xa;            &#x27;1970-01-01&#x27;, &#xd;&#xa;            t4.i&#x2a;10000 &#x2b; t3.i&#x2a;1000 &#x2b; t2.i&#x2a;100 &#x2b; t1.i&#x2a;10 &#x2b; t0.i&#xd;&#xa;        &#x29; selected_date &#xd;&#xa;    FROM&#xd;&#xa;&#x28;&#xd;&#xa;    SELECT 0 i &#xd;&#xa;        UNION &#xd;&#xa;    SELECT 1 &#xd;&#xa;        UNION &#xd;&#xa;    SELECT 2 &#xd;&#xa;        UNION &#xd;&#xa;    SELECT 3 &#xd;&#xa;        UNION &#xd;&#xa;    SELECT 4 &#xd;&#xa;        UNION &#xd;&#xa;    SELECT 5 &#xd;&#xa;        UNION &#xd;&#xa;    SELECT 6 &#xd;&#xa;        UNION &#xd;&#xa;    SELECT 7 &#xd;&#xa;        UNION &#xd;&#xa;    SELECT 8 &#xd;&#xa;        UNION &#xd;&#xa;    SELECT 9&#xd;&#xa;&#x29; t0,&#xd;&#xa; &#x28;SELECT 0 i UNION SELECT 1 UNION SELECT 2 UNION SELECT 3 UNION SELECT 4 UNION SELECT 5 UNION SELECT 6 UNION SELECT 7 UNION SELECT 8 UNION SELECT 9&#x29; t1,&#xd;&#xa; &#x28;SELECT 0 i UNION SELECT 1 UNION SELECT 2 UNION SELECT 3 UNION SELECT 4 UNION SELECT 5 UNION SELECT 6 UNION SELECT 7 UNION SELECT 8 UNION SELECT 9&#x29; t2,&#xd;&#xa; &#x28;SELECT 0 i UNION SELECT 1 UNION SELECT 2 UNION SELECT 3 UNION SELECT 4 UNION SELECT 5 UNION SELECT 6 UNION SELECT 7 UNION SELECT 8 UNION SELECT 9&#x29; t3,&#xd;&#xa; &#x28;SELECT 0 i UNION SELECT 1 UNION SELECT 2 UNION SELECT 3 UNION SELECT 4 UNION SELECT 5 UNION SELECT 6 UNION SELECT 7 UNION SELECT 8 UNION SELECT 9&#x29; t4&#x29; v&#xd;&#xa;WHERE selected_date BETWEEN DATE&#x28;&#x27;2000-02-10&#x27;&#x29; and DATE&#x28;&#x27;2030-02-15&#x27;&#x29;&#x3b;</sql>
